{
  "name": "clean-code-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --passWithNoTests --no-cache --runInBand --watchAll=false",
    "test:watch": "yarn test --watchAll",
    "test:staged": "yarn test --findRelatedTests",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn jest --ci --coverage",
    "prepare:husky": "husky install"
  },
  "dependencies": {
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@ngneat/falso": "^7.1.1",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "axios": "^1.6.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5"
  }
}
